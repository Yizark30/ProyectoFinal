<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".informacionPropuesta">

    <RelativeLayout android:id="@+id/content_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <RelativeLayout android:id="@+id/page_informacion_de_una_propuesta_ek1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >

            <View
                android:id="@+id/_bg__informacion_de_una_propuesta_ek2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/_bg__informacion_de_una_propuesta_ek2_shape" />


            <TextView
                android:id="@+id/informacion_de_la_propuesta"
                android:layout_width="357dp"
                android:layout_height="80dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="68dp"
                android:gravity="center"
                android:text="Informacion de la propuesta"
                android:textColor="@color/vista_de_respuestas_color"
                android:textSize="24sp"
                android:textStyle="bold" />


            <RelativeLayout
                android:id="@+id/group_5_ek1"
                android:layout_marginTop="670dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                >

                <View
                    android:id="@+id/rectangle_6_ek4"
                    android:layout_width="420dp"
                    android:layout_height="match_parent"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/rectangle_6_ek4_shape" />

                <ImageView
                    android:id="@+id/image_6_ek2"
                    android:layout_width="68dp"
                    android:layout_height="63dp"
                    android:layout_marginStart="175dp"
                    android:layout_marginTop="5dp"
                    android:contentDescription="image_6_ek2"
                    android:src="@drawable/image_6_ek2" />

                <ImageView
                    android:id="@+id/_avatar_1_ek2"
                    android:layout_width="61dp"
                    android:layout_height="64dp"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="5dp"
                    android:contentDescription="_avatar_1_ek2"
                    android:src="@drawable/_avatar_1_ek2" />

                <ImageView
                    android:id="@+id/_image_5_ek3"
                    android:layout_width="68dp"
                    android:layout_height="63dp"
                    android:layout_marginStart="300dp"
                    android:layout_marginTop="5dp"
                    android:contentDescription="_image_5_ek3"
                    android:src="@drawable/_image_5_ek3" />

            </RelativeLayout>

            <ImageView
                android:id="@+id/_image_5_ek2"
                android:layout_width="97.35dp"
                android:layout_height="64dp"
                android:layout_marginStart="160dp"
                android:layout_marginTop="560dp"
                android:contentDescription="_image_5_ek2"
                android:src="@drawable/_image_5_ek2" />

        </RelativeLayout>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>